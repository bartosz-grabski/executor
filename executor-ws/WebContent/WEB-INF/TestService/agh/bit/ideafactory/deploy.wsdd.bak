<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TestService WSDL service -->

  <service name="Test" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ideafactory.bit.agh"/>
      <parameter name="wsdlServiceElement" value="TestService"/>
      <parameter name="schemaQualified" value="http://ideafactory.bit.agh"/>
      <parameter name="wsdlServicePort" value="Test"/>
      <parameter name="className" value="agh.bit.ideafactory.TestSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Test"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sayHello" qname="operNS:sayHello" xmlns:operNS="http://ideafactory.bit.agh" returnQName="retNS:sayHelloReturn" xmlns:retNS="http://ideafactory.bit.agh" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://ideafactory.bit.agh" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="sayDupa" qname="operNS:sayDupa" xmlns:operNS="http://ideafactory.bit.agh" returnQName="retNS:sayDupaReturn" xmlns:retNS="http://ideafactory.bit.agh" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:whatever" xmlns:pns="http://ideafactory.bit.agh" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="sayDupa sayHello"/>

  </service>
</deployment>
